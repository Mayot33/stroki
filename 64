def rim(s):
    #словарь
    alf = ['I_1', 'V_5', 'X_10', 'L_50', 'C_100', 'D_500', 'M_1000']
    for i in range (len(s)-1):
       c1 = s[i]
        for j in alf:
            if j[0] ==  c1:
                c1 = j[2:]
